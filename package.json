{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.14.1"
  },
  "scripts": {
    "start": "npm run prod:buildserver; node ./server/server-dist/",
    "upgrade-interactive": "npm-check --update",
    "server": "nodemon ./server/index.js --exec babel-node",
    "prod:buildserver": "rm -rf ./server/server-dist && babel ./server -d ./server/server-dist --copy-files",
    "client": "npm run start --prefix client",
    "audit": "npm audit fix && cd client/ && npm audit fix && cd ..",
    "getAll": "npm i && cd client/ && npm i && cd ..",
    "dev": "concurrently --raw --kill-others \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "lint": "eslint \"./**/*.{js,jsx}\" --quiet",
    "precommit": "lint-staged"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@chakra-ui/core": "^0.5.2",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "autoprefixer": "^9.7.4",
    "axios": "^0.19.2",
    "babel-preset-react": "6.24.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-session": "^1.4.0",
    "emotion-theming": "^10.0.27",
    "jest": "^25.1.0",
    "materialize-css": "^1.0.0",
    "postcss-loader": "^3.0.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.2.0",
    "file-loader": "^5.0.2",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack-dev-server": "^3.10.2",
    "webpack-manifest-plugin": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "express": "^4.17.1",
    "mongoose": "^5.8.11",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "npm-check": "^5.9.0",
    "prettier": "^1.19.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
